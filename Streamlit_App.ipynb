      "source": [
        "%%writefile app.py\n",
        "import streamlit as st\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import joblib\n",
        "import os\n",
        "from io import BytesIO\n",
        "from datetime import datetime\n",
        "from zoneinfo import ZoneInfo  # Python 3.9+\n",
        "\n",
        "# ---------------- UI SETUP ----------------\n",
        "st.set_page_config(page_title=\"HDB Price Predictor\", page_icon=\"üè†\", layout=\"centered\")\n",
        "st.title(\"üè† HDB Resale Price Prediction (Linear Pipeline)\")\n",
        "\n",
        "# ----------- DEFAULT PATHS (fallbacks) -----------\n",
        "DEFAULT_MODEL_PATH = \"/content/hdb_price_pipeline.pkl\"\n",
        "DEFAULT_DATA_PATH  = \"/content/hdb_processed_data.csv\"  # processed training CSV (with 'resale_price')\n",
        "\n",
        "# ----------- SIDEBAR: optional uploads -----------\n",
        "st.sidebar.header(\"‚öôÔ∏è Options\")\n",
        "st.sidebar.caption(\"Upload to override defaults. If empty, the defaults are used.\")\n",
        "uploaded_model = st.sidebar.file_uploader(\"Model pipeline (.pkl)\", type=[\"pkl\"])\n",
        "uploaded_data  = st.sidebar.file_uploader(\"Training CSV (for feature schema)\", type=[\"csv\"])\n",
        "\n",
        "# ---- Student / Project Metadata ----\n",
        "st.sidebar.header(\"üë§ Student Metadata\")\n",
        "student_name = st.sidebar.text_input(\"Name\", value=\"Aung Hlaing Tun\")\n",
        "student_id = st.sidebar.text_input(\"Student ID\", value=\"6319250G\")\n",
        "course = st.sidebar.text_input(\"Course\", value=\"ITI-105\")\n",
        "team_id = st.sidebar.text_input(\"Project Group ID\", value=\"AlogoRiddler\")\n",
        "project_date = st.sidebar.text_input(\"Project Date\", value=\"25 Aug 2025\")\n",
        "sg_now = datetime.now(ZoneInfo(\"Asia/Singapore\"))\n",
        "inference_ts = st.sidebar.text_input(\"Model Inference Date (SGT)\", value=sg_now.strftime(\"%Y-%m-%d %H:%M:%S %Z\"))\n",
        "\n",
        "# ----------- Paths after optional uploads -----------\n",
        "MODEL_PATH = DEFAULT_MODEL_PATH\n",
        "DATA_PATH  = DEFAULT_DATA_PATH\n",
        "if uploaded_model is not None:\n",
        "    MODEL_PATH = \"/content/_tmp_model.pkl\"\n",
        "    with open(MODEL_PATH, \"wb\") as f:\n",
        "        f.write(uploaded_model.read())\n",
        "if uploaded_data is not None:\n",
        "    DATA_PATH = \"/content/_tmp_data.csv\"\n",
        "    with open(DATA_PATH, \"wb\") as f:\n",
        "        f.write(uploaded_data.read())\n",
        "\n",
        "# ----------- Helpers -----------\n",
        "def clamp(val, lo, hi, fallback=None):\n",
        "    \"\"\"Clamp numeric default into [lo, hi]. If val is nan/None, use fallback or lo.\"\"\"\n",
        "    if val is None or (isinstance(val, float) and np.isnan(val)):\n",
        "        return fallback if fallback is not None else lo\n",
        "    try:\n",
        "        v = float(val)\n",
        "    except Exception:\n",
        "        return fallback if fallback is not None else lo\n",
        "    if v < lo: return lo\n",
        "    if v > hi: return hi\n",
        "    return v\n",
        "\n",
        "def clamp_int(val, lo, hi, fallback=None):\n",
        "    v = clamp(val, lo, hi, fallback if fallback is not None else lo)\n",
        "    return int(round(v))\n",
        "\n",
        "@st.cache_resource(show_spinner=False)\n",
        "def load_model(path: str):\n",
        "    return joblib.load(path)\n",
        "\n",
        "@st.cache_data(show_spinner=False)\n",
        "def load_template(path: str):\n",
        "    df = pd.read_csv(path, nrows=1)\n",
        "    X = df.drop(columns=[\"resale_price\"]) if \"resale_price\" in df.columns else df.copy()\n",
        "    return X, list(X.columns)\n",
        "\n",
        "@st.cache_data(show_spinner=False)\n",
        "def get_category_choices(csv_path: str, available_cols, cols=(\"town\",\"flat_type\",\"flat_model\",\"storey_range\")):\n",
        "    usecols = [c for c in cols if c in available_cols]\n",
        "    if not usecols:\n",
        "        return {}\n",
        "    df = pd.read_csv(csv_path, usecols=usecols)\n",
        "    choices = {c: sorted(df[c].dropna().astype(str).unique().tolist()) for c in df.columns}\n",
        "    return choices\n",
        "\n",
        "# ----------- Load model + template row -----------\n",
        "if not os.path.exists(MODEL_PATH):\n",
        "    st.error(f\"Model pipeline not found at {MODEL_PATH}\")\n",
        "    st.stop()\n",
        "if not os.path.exists(DATA_PATH):\n",
        "    st.error(f\"Training CSV not found at {DATA_PATH}\")\n",
        "    st.info(\"Upload the exact CSV you trained with (includes 'resale_price').\")\n",
        "    st.stop()\n",
        "\n",
        "pipe = load_model(MODEL_PATH)\n",
        "X_template, required_cols = load_template(DATA_PATH)\n",
        "choices = get_category_choices(DATA_PATH, available_cols=required_cols)\n",
        "\n",
        "# metadata pill under the title\n",
        "st.markdown(\n",
        "    f\"\"\"\n",
        "    <div style=\"padding:10px;border-radius:8px;background:#F7F9FC;border:1px solid #E6EAF2;margin-bottom:8px\">\n",
        "      <strong>Name:</strong> {student_name} &nbsp;|&nbsp;\n",
        "      <strong>ID:</strong> {student_id} &nbsp;|&nbsp;\n",
        "      <strong>Course:</strong> {course} &nbsp;|&nbsp;\n",
        "      <strong>Group:</strong> {team_id} &nbsp;|&nbsp;\n",
        "      <strong>Project Date:</strong> {project_date} &nbsp;|&nbsp;\n",
        "      <strong>Inference:</strong> {inference_ts}\n",
        "    </div>\n",
        "    \"\"\",\n",
        "    unsafe_allow_html=True,\n",
        ")\n",
        "\n",
        "st.caption(f\"Model expects **{len(required_cols)} features**. Using a template row to match columns & order.\")\n",
        "\n",
        "# handy getters\n",
        "def get_num(name, default):\n",
        "    return float(X_template[name].iloc[0]) if name in X_template.columns else default\n",
        "def get_str(name, default):\n",
        "    return str(X_template[name].iloc[0]) if name in X_template.columns else default\n",
        "\n",
        "# ----------- UI: Numeric inputs (clamped) -----------\n",
        "col1, col2 = st.columns(2)\n",
        "with col1:\n",
        "    floor_area_sqm = st.number_input(\"Floor Area (sqm)\", 20.0, 250.0,\n",
        "                                     clamp(get_num(\"floor_area_sqm\", 90.0), 20.0, 250.0, 90.0), step=1.0)\n",
        "    remaining_lease_years = st.number_input(\"Remaining Lease (years)\", 1.0, 99.0,\n",
        "                                            clamp(get_num(\"remaining_lease_years\", 70.0), 1.0, 99.0, 70.0), step=1.0)\n",
        "    min_storey = st.number_input(\"Min Storey\", 1, 60, clamp_int(get_num(\"min_storey\", 1), 1, 60, 1), step=1)\n",
        "    max_storey = st.number_input(\"Max Storey\", 1, 60, clamp_int(get_num(\"max_storey\", 3), 1, 60, 3), step=1)\n",
        "with col2:\n",
        "    latitude = st.number_input(\"Latitude\", 1.20, 1.50,\n",
        "                               clamp(get_num(\"latitude\", 1.3500), 1.20, 1.50, 1.3500), step=0.0001, format=\"%.6f\")\n",
        "    longitude = st.number_input(\"Longitude\", 103.60, 104.10,\n",
        "                                clamp(get_num(\"longitude\", 103.8500), 103.60, 104.10, 103.8500), step=0.0001, format=\"%.6f\")\n",
        "    cpi = st.number_input(\"CPI\", 80.0, 140.0,\n",
        "                          clamp(get_num(\"cpi\", 105.0), 80.0, 140.0, 105.0), step=0.1)\n",
        "    distance_to_mrt = st.number_input(\"Distance to MRT (km)\", 0.0, 20.0,\n",
        "                                      clamp(get_num(\"distance_to_mrt\", 1.2), 0.0, 20.0, 1.2), step=0.1)\n",
        "\n",
        "year_default = clamp_int(get_num(\"year\", 2024), 2015, 2025, 2024)\n",
        "year = st.number_input(\"Transaction Year\", 2015, 2025, year_default, step=1)\n",
        "mn_default = clamp_int(get_num(\"month_num\", 1), 1, 12, 1)\n",
        "month_num = st.selectbox(\"Transaction Month (1‚Äì12)\", list(range(1, 13)), index=mn_default - 1)\n",
        "\n",
        "# ensure storey consistency\n",
        "if max_storey < min_storey:\n",
        "    st.info(\"`Max Storey` cannot be less than `Min Storey`. Adjusting to match.\")\n",
        "    max_storey = min_storey\n",
        "mid_storey_val = (min_storey + max_storey) / 2.0\n",
        "\n",
        "# ----------- UI: Categoricals (dropdowns if available, else text) -----------\n",
        "def cat_input(label, col_name, default):\n",
        "    opts = choices.get(col_name, [])\n",
        "    if col_name in required_cols and opts:\n",
        "        return st.selectbox(label, options=opts, index=0)\n",
        "    return st.text_input(label, value=default)\n",
        "\n",
        "town         = cat_input(\"Town\", \"town\", get_str(\"town\", \"ANG MO KIO\"))\n",
        "flat_type    = cat_input(\"Flat Type\", \"flat_type\", get_str(\"flat_type\", \"3 ROOM\"))\n",
        "flat_model   = cat_input(\"Flat Model\", \"flat_model\", get_str(\"flat_model\", \"Model A\"))\n",
        "storey_range = cat_input(\"Storey Range\", \"storey_range\", get_str(\"storey_range\", \"01 TO 03\"))\n",
        "\n",
        "st.caption(\"Tip: Use the same formatting as your dataset (e.g., 'ANG MO KIO', '3 ROOM', 'Model A', '01 TO 03').\")\n",
        "\n",
        "# ----------- Debug / Info -----------\n",
        "with st.expander(\"üîé Debug / Expected columns\", expanded=False):\n",
        "    st.write(f\"Columns seen by pipeline: {len(required_cols)}\")\n",
        "    if st.checkbox(\"Show full required column list\"):\n",
        "        st.code(\"\\n\".join(required_cols), language=\"text\")\n",
        "\n",
        "show_row = st.checkbox(\"Show feature row sent to model\", value=False)\n",
        "\n",
        "# ----------- Predict -----------\n",
        "pred_val = None\n",
        "if st.button(\"Predict Price\"):\n",
        "    X_input = X_template.copy()\n",
        "\n",
        "    def set_if_present(col, val):\n",
        "        if col in X_input.columns:\n",
        "            X_input.loc[:, col] = val\n",
        "\n",
        "    # numeric overrides\n",
        "    set_if_present(\"floor_area_sqm\", floor_area_sqm)\n",
        "    set_if_present(\"remaining_lease_years\", remaining_lease_years)\n",
        "    set_if_present(\"min_storey\", min_storey)\n",
        "    set_if_present(\"max_storey\", max_storey)\n",
        "    if \"mid_storey\" in X_input.columns:\n",
        "        set_if_present(\"mid_storey\", mid_storey_val)\n",
        "    if \"property_age\" in X_input.columns:\n",
        "        set_if_present(\"property_age\", X_input[\"property_age\"].iloc[0])  # keep template unless you add a control\n",
        "    set_if_present(\"latitude\", latitude)\n",
        "    set_if_present(\"longitude\", longitude)\n",
        "    set_if_present(\"cpi\", cpi)\n",
        "    set_if_present(\"distance_to_mrt\", distance_to_mrt)\n",
        "    set_if_present(\"year\", year)\n",
        "    set_if_present(\"month_num\", int(month_num))\n",
        "\n",
        "    # categoricals\n",
        "    set_if_present(\"town\", town)\n",
        "    set_if_present(\"flat_type\", flat_type)\n",
        "    set_if_present(\"flat_model\", flat_model)\n",
        "    set_if_present(\"storey_range\", storey_range)\n",
        "\n",
        "    # ensure required order\n",
        "    X_input = X_input[required_cols]\n",
        "\n",
        "    try:\n",
        "        pred_val = float(pipe.predict(X_input)[0])\n",
        "        st.success(f\"Predicted Resale Price: **${pred_val:,.0f}**\")\n",
        "        st.info(\n",
        "            f\"Name: {student_name} | ID: {student_id} | Course: {course} | \"\n",
        "            f\"Group: {team_id} | Project Date: {project_date} | Inference (SGT): {inference_ts}\"\n",
        "        )\n",
        "        if show_row:\n",
        "            st.dataframe(X_input)\n",
        "    except Exception as e:\n",
        "        st.error(f\"Prediction failed: {e}\")\n",
        "        st.code(f\"Expected columns ({len(required_cols)}):\\n{required_cols}\")\n",
        "\n",
        "# ----------- Download: row + prediction -----------\n",
        "if pred_val is not None:\n",
        "    out = X_input.copy()\n",
        "    out.insert(0, \"predicted_resale_price\", round(pred_val, 2))\n",
        "    # attach student metadata\n",
        "    out.insert(1, \"student_name\", student_name)\n",
        "    out.insert(2, \"student_id\", student_id)\n",
        "    out.insert(3, \"course\", course)\n",
        "    out.insert(4, \"project_group_id\", team_id)\n",
        "    out.insert(5, \"project_date\", project_date)\n",
        "    out.insert(6, \"inference_timestamp_sgt\", inference_ts)\n",
        "    csv_bytes = out.to_csv(index=False).encode(\"utf-8\")\n",
        "    st.download_button(\n",
        "        \"‚¨áÔ∏è Download input row + prediction (CSV)\",\n",
        "        data=csv_bytes, file_name=\"hdb_prediction_row.csv\", mime=\"text/csv\",\n",
        "    )\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
